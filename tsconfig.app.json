{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tsbuildinfo",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": true,
    "checkJs": true,
    "jsx": "react-jsx", // Configuração crucial para JSX
    "types": ["vite/client"]
  },
  "include": ["src", "src/vite-env.d.ts"],
  "exclude": ["node_modules"]
}